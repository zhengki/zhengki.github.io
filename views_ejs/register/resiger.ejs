<!doctype html>
<html>

<%- include("../public/head.ejs",{
title:"注册"
}) %>
<link rel="stylesheet" href="../../css/resiger/resiger.css">


</head>

<body>
<%- include("../public/header.ejs",{active:0}) %>
    <div class="content">
        <div class="breadcrumb">
            <span class="layui-breadcrumb">
            <a href="../../views/index/index.html">首页</a>
            <a href="#">注册</a>
        </span>
        </div>
        <div class="content-title">
            <a href=""><span>会员注册</span></a>
        </div>
        <div class="form-details">
            <form action="" class="layui-form">
                <div class="layui-form-item">
                    <label class="layui-form-label">身份:</label>
                    <div class="layui-input-block">
                        <input type="radio" name="sex1" value="男" title="企业">
                        <input type="radio" name="sex1" value="女" title="个人" checked>
                        <input type="radio" name="sex1" value="女" title="政府/事业单位" >
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">账号类型:</label>
                    <div class="layui-input-block">
                        <input type="radio" name="sex2" value="男" title="资金方">
                        <input type="radio" name="sex2" value="女" title="项目方" checked>
                        <input type="radio" name="sex2" value="女" title="施工方" >
                        <input type="radio" name="sex2" value="女" title="其他" >
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">手机号:</label>
                    <div class="layui-input-block">
                        <input type="text" name="title" required  lay-verify="required" placeholder="输入您的手机号码" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">邮箱:</label>
                    <div class="layui-input-block">
                        <input type="text" name="title" required  lay-verify="required" placeholder="输入您的常用邮箱" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">登录密码:</label>
                    <div class="layui-input-block">
                        <input type="text" name="title" required  lay-verify="required" placeholder="输入6-16位数字与字母的组合" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">确认密码:</label>
                    <div class="layui-input-block">
                        <input type="text" name="title" required  lay-verify="required" placeholder="再次输入新置密码" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label">验证码:</label>
                    <div class="layui-input-block">
                        <input type="text" name="title" required  lay-verify="required" placeholder="输入手机验证码" autocomplete="off" class="layui-input authcode">
                        <a href="" class="layui-btn">获取验证码</a>
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label"></label>
                    <div class="layui-input-block">
                        <input type="checkbox" name="" lay-skin="primary" checked>同意《统云服务协议》
                    </div>
                </div>
                <div class="layui-form-item">
                    <label class="layui-form-label"></label>
                    <div class="layui-input-block">
                        <input type="submit" name=""  value="立即注册">
                    </div>
                </div>
                <div class="login">
                    已有账号， <a href="../../views/index/index.html">立即登录</a>
                </div>
            </form>
        </div>
    </div>







<%- include("../public/footer.ejs") %>
</body>

</html>